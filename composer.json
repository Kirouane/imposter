{
    "name": "kirouane/imposter",
    "description": "API mock for PHPUnit",
    "type": "library",
    "minimum-stability":"dev",

    "homepage": "https://github.com/Kirouane/interval",
    "license": "MIT",
    "authors": [
        {
            "name": "nassim.kirouane",
            "email": "nassim.kirouane@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/Kirouane/interval/issues"
    },
    "autoload": {
        "psr-4": {
            "Imposter\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imposter\\" : ["test/unit", "test/api"]
        }
    },
    "require": {
        "php" : ">=7.0",
        "ext-json" :"*",
        "guzzlehttp/guzzle": "^6.3@dev",
        "react/http": "dev-master",
        "phpunit/phpunit": "^7.4@dev",
        "mockery/mockery": "^1.0@dev",
        "phpro/grumphp": "dev-master",
        "symfony/console": "^4.2@dev",
        "maglnet/composer-require-checker": "^1.1@dev",
        "sebastian/phpcpd": "^4.0@dev",
        "php-coveralls/php-coveralls": "2.1.x-dev",
        "friendsofphp/php-cs-fixer": "^2.13@dev",
        "jakub-onderka/php-parallel-lint": "dev-master",
        "squizlabs/php_codesniffer": "^3.0@dev",
        "phpmd/phpmd": "dev-master",
        "phpstan/phpstan": "^0.10.0@dev",
        "povils/phpmnd": "^2.0@dev",
        "sensiolabs/security-checker": "^5.0@dev"
    }

}

{
    "name": "kirouane/imposter",
    "description": "HTTP Mock and Stub",
    "type": "library",
    "homepage": "https://github.com/Kirouane/imposter",
    "license": "MIT",
    "prefer-stable": true,
    "authors": [
        {
            "name": "nassim.kirouane",
            "email": "nassim.kirouane@gmail.com",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/Kirouane/imposter/issues"
    },
    "autoload": {
        "psr-4": {
            "Imposter\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imposter\\" : ["test/unit", "test/api"]
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        }
    },
    "require": {
        "php" : ">=7.0",
        "ext-json" :"*",
        "symfony/console": ">= 2, < 5",
        "symfony/templating": ">= 2, < 5",
        "monolog/monolog": ">= 1,  < 2",
        "guzzlehttp/guzzle": ">= 6, < 7 ",
        "react/http": "^0.8.3",
        "php-di/php-di": "^6.0",
        "phpunit/phpunit": "^6"
    },
    "require-dev": {
        "php-coveralls/php-coveralls": "2.1.x-dev",
        "mockery/mockery": "^1.0@dev"
    }
}
